{
	"name": "pampers-id-binding-server",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"server": "babel-node --inspect ./src/bin/www",
		"dev": "set NODE_ENV=development && npm-run-all server",
		"clean": "rimraf dist",
		"build": "babel ./src --out-dir dist",
		"server:prod": "node ./dist/bin/www",
		"prod": "set NODE_ENV=production && npm-run-all clean build server:prod",
		"watch": "nodemon",
		"auto-migrate-database": "sequelize-auto -h 127.0.0.1 -d pampers-backend-dev -u root -x password -p 3306 -o ./src/models -e mysql"
	},
	"dependencies": {
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-prettier": "^3.1.0",
		"express": "~4.16.0",
		"morgan": "~1.9.0",
		"mysql2": "^1.6.5",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.17.1",
		"rimraf": "^2.6.3",
		"sequelize": "^5.15.1",
		"sequelize-cli": "^5.4.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/node": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.17.3",
		"nodemon": "^1.19.1"
	}
}
