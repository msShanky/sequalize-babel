{
	"name": "pampers-id-binding-server",
	"version": "0.0.0",
	"private": true,
	"engines": {
		"node": "v10.15.0",
		"npm": "6.4.1"
	},
	"scripts": {
		"start": "node ./dist/bin/www",
		"server": "babel-node --inspect ./src/bin/www",
		"dev": "set NODE_ENV=development && npm-run-all server",
		"clean": "rimraf dist",
		"build": "babel ./src --out-dir dist",
		"server:prod": "node ./dist/bin/www",
		"build-dev": "set NODE_ENV=development && npm-run-all clean build",
		"build-qa": "set NODE_ENV=qa && npm-run-all clean build",
		"build-prod": "set NODE_ENV=production && npm-run-all clean build",
		"prod": "set NODE_ENV=production && npm-run-all clean build server:prod",
		"watch": "nodemon",
		"auto-migrate-database": "sequelize-auto -h 127.0.0.1 -d pampers-backend-dev -u root -x password -p 3306 -o ./src/models -e mysql"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cookie-parser": "~1.4.3",
		"core-js": "^3.1.3",
		"debug": "~2.6.9",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-prettier": "^3.1.0",
		"express": "~4.16.0",
		"morgan": "~1.9.0",
		"mysql2": "^1.6.5",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.17.1",
		"rimraf": "^2.6.3",
		"sequelize": "^5.8.7",
		"sequelize-cli": "^5.4.0",
		"tedious": "^6.1.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/node": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-export-default-from": "^7.2.0",
		"@babel/plugin-transform-runtime": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/runtime": "^7.4.5",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.0.6",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.17.3",
		"nodemon": "^1.19.1"
	}
}
